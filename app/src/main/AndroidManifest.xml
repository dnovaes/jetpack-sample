<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.arctouch.io.outdoorsychallenge">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".OutdoorsyApplication"
        android:allowBackup="false"
        android:icon="@drawable/icon_car"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/icon_car"
        android:supportsRtl="true"
        android:theme="@style/OutdoorsyTheme">
        <activity android:name=".features.main.OutdoorsyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>